https://www.codechef.com/problems/WEEDING?tab=statement
#include <stdio.h>

int main(void) 
{
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n,m,k;
	    scanf("%d %d %d",&n,&m,&k);
	    int arr[n];
	    int count=0;
	    for(int i=0;i<n;i++)
	    {
	        scanf("%d",&arr[i]);
	        if((m-arr[i])+1 >= k)
	        {
	            count++;
	        }
	    }
	    if(count == n)
	    {
	        printf("Yes\n");
	    }
	    else
	    {
	        printf("No\n");
	    }
	    
	    
	    
	}

}

