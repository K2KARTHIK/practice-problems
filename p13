https://www.codechef.com/problems/ELECTIONS
#include<stdio.h>
int main()
{
    int a,b,c,t;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d %d %d",&a,&b,&c);
        if(a>50 && b<=50 && c<=50)
        {
            printf("A\n");
        }
        else if(a<=50 && b>50 && c<=50)
        {
            printf("B\n");
        }
        else if(a<=50 && b<=50 && c>50)
        {
            printf("C\n");
        }
        else
        {
            printf("NOTA\n");
        }
    }
    return 0;
}
