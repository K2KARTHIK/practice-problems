https://www.codechef.com/problems/CHEFSTEP?tab=statement
#include <stdio.h>

int main() {
    int t, n, k;
    scanf("%d", &t);

    while (t--) {
        scanf("%d%d", &n, &k);
        int a[n];

        for (int i = 0; i < n; i++) {
            scanf("%d", &a[i]);
        }

        for (int i = 0; i < n; i++) {
            if (a[i] % k == 0) {
                printf("1");
            } else {
                printf("0");
            }
        }

        printf("\n");
    }

    return 0;
}
