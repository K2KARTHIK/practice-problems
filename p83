https://www.codechef.com/START138D/problems/LONGDRIVE
#include <stdio.h>
#include <math.h>

int main() {
    int T;
    scanf("%d", &T);
    while (T--) {
        int X, Y;
        scanf("%d %d", &X, &Y);
        
        if (Y == 100) {
            printf("-1\n");
            continue;
        }
        
        double required_hours = (10.0 * (Y - X)) / (100 - Y);
        int h = (int)ceil(required_hours);
        
        printf("%d\n", h);
    }
    return 0;
}
