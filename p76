https://www.codechef.com/problems/DIVIDING?tab=statement\
#include <stdio.h>

int main() {
    long int n, sum = 0;
    scanf("%ld", &n);
    int a[n];

    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        sum += a[i];
    }

    if (sum == (n * (n + 1)) / 2) {
        printf("YES\n");
    } else {
        printf("NO\n");
    }
    return 0;
}
