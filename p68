https://www.codechef.com/problems/SRTARR?tab=statement
#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n,i,c=0;
	    scanf("%d",&n);
	    char s[n];
	    scanf("%s",s);
	    for(i=0;i<n;i++)
	    {
	        if(s[i]=='1'&&s[i+1]=='0')
	            c++;
	    }
	    printf("%d\n",c);
	}
	return 0;
}
