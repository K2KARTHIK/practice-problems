https://www.codechef.com/problems/MAGICHF?tab=statement
#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int a,b,c,e,f;
	    scanf("%d%d%d",&a,&b,&c);
	    while(c--){
	        scanf("%d%d",&e,&f);
	        if(e==b)
	        {
	            b=f;
	        }
	        else if(f==b)
	        {
	            b=e;
	        }
	    }
	    printf("%d\n",b);
	}
	return 0;
}
