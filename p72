https://www.codechef.com/problems/CLLCM?tab=statement
#include <stdio.h>

int main(void)
{
    int T;
    scanf("%d",&T);
    while(T--)
    {
        int n,flag=0;
        scanf("%d",&n);
        int arr[n];
        for(int i=0;i<n;i++)
        {
            scanf("%d",&arr[i]);
            if(arr[i]%2==0)
            flag = 1;
        }
        if(flag == 1)
        {
            printf("NO\n");
        }
        else
        {
            printf("YES\n");
            
        }
    }
    return 0;

}
